#breadcrumb
  = raw "#{link_to('Clients', admin_clients_path)} &raquo; #{link_to(@client.name + ' Bookings', admin_client_bookings_path(@client))} &raquo; #{link_to @booking.title + ' Photos', collection_path} &raquo; Editing #{@booking_photo.title}"
  
- form_for(@booking_photo, :url => resource_path, :html => { :multipart => true, :method => :put }) do |f|
  = render :partial => "form", :locals => { :f => f }
